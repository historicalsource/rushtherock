1.27
log
@Corrected definitions and use of cache instruction defines.
@
text
@d15 2
a16 5
.if true, (HOST_BOARD == PHOENIX) || (HOST_BOARD == PHOENIX_AD) || (HOST_BOARD == FLAGSTAFF) || (HOST_BOARD == SEATTLE) || (HOST_BOARD == VEGAS)
  HB_PHOENIX == 1
.iff
  HB_PHOENIX == 0
.endc
d174 1
d255 3
d259 1
@


1.26
log
@Syntax for gas 2.7 is only one name on a .global directive.
Split all .globl statements into multiples where appropriate.
gas 2.7 doesn't like numbers appearing as the first item after
a # in (effectively) column 1. It assumes it is a line number.
@
text
@d202 19
a220 26
	HEX_CONST	Index_Invalidate_I               0x0         < * 0 0 >
	HEX_CONST	Index_Writeback_Inv_D            0x1         < * 0 1 >
	HEX_CONST	Index_Invalidate_SI              0x2         < * 0 2 >
	HEX_CONST	Index_Writeback_Inv_SD           0x3         < * 0 3 >
	HEX_CONST	Index_Load_Tag_I                 0x4         < * 1 0 >
	HEX_CONST	Index_Load_Tag_D                 0x5         < * 1 1 >
	HEX_CONST	Index_Load_Tag_SI                0x6         < * 1 2 >
	HEX_CONST	Index_Load_Tag_SD                0x7         < * 1 3 >
	HEX_CONST	Index_Store_Tag_I                0x8         < * 2 0 >
	HEX_CONST	Index_Store_Tag_D                0x9         < * 2 1 >
	HEX_CONST	Index_Store_Tag_SI               0xA         < * 2 2 >
	HEX_CONST	Index_Store_Tag_SD               0xB         < * 2 3 >
	HEX_CONST	Create_Dirty_Exc_D               0xD         < * 3 1 >
	HEX_CONST	Create_Dirty_Exc_SD              0xF         < * 3 3 >
	HEX_CONST	Hit_Invalidate_I                 0x10        < * 4 0 >
	HEX_CONST	Hit_Invalidate_D                 0x11        < * 4 1 >
	HEX_CONST	Hit_Invalidate_SI                0x12        < * 4 2 >
	HEX_CONST	Hit_Invalidate_SD                0x13        < * 4 3 >
	HEX_CONST	Hit_Writeback_Inv_D              0x15        < * 5 1 >
	HEX_CONST	Hit_Writeback_Inv_SD             0x17        < * 5 3 >
	HEX_CONST	Fill_I                           0x14        < * 5 0 >
	HEX_CONST	Hit_Writeback_D                  0x19        < * 6 1 >
	HEX_CONST	Hit_Writeback_SD                 0x1B        < * 6 3 >
	HEX_CONST	Hit_Writeback_I                  0x18        < * 6 0 >
	HEX_CONST	Hit_Set_Virtual_SI               0x1E        < * 7 2 >
	HEX_CONST	Hit_Set_Virtual_SD               0x1F        < * 7 3 >
@


